<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Pet.TrainingEventController">
    <TabPane prefHeight="720.0" prefWidth="1280.0" tabClosingPolicy="UNAVAILABLE">
        <Tab text="Training">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <HBox layoutX="401.0" layoutY="-1.0" prefHeight="693.0" prefWidth="879.0">
                            <children>
                                <TableView fx:id="myTrainingEventTable" prefHeight="693.0" prefWidth="880.0">
                                    <columns>
                                        <TableColumn fx:id="myEventIDCol" prefWidth="175.0" text="EventID" />
                                        <TableColumn fx:id="myTrainingTypeCol" prefWidth="170.0" text="Training Type" />
                                        <TableColumn fx:id="myExecutionTimeCol" minWidth="0.0" prefWidth="140.0" text="Execution Time (s)" />
                                        <TableColumn fx:id="myCommandHeldTimeCol" minWidth="0.0" prefWidth="152.0" text="Command Held Time (s)" />
                                        <TableColumn fx:id="myNotesCol" minWidth="0.0" prefWidth="241.0" text="Notes" />
                                    </columns>
                                </TableView>
                            </children>
                        </HBox>
                        <VBox prefHeight="230.0" prefWidth="402.0" AnchorPane.leftAnchor="0.0">
                            <children>
                                <Label prefHeight="17.0" prefWidth="410.0" text="Add Training" />
                                <ComboBox fx:id="myTrainingType" prefHeight="25.0" prefWidth="372.0" promptText="Training Type" />
                                <TextField promptText="Execution Time (s)" />
                                <TextField promptText="Command Held Time (s)" />
                                <TextArea prefHeight="75.0" prefWidth="372.0" promptText="Notes" />
                                <Button mnemonicParsing="false" onAction="#loadTrainingEventData" prefHeight="25.0" prefWidth="379.0" text="Log Training">
                                    <VBox.margin>
                                        <Insets top="10.0" />
                                    </VBox.margin>
                                </Button>
                            </children>
                            <padding>
                                <Insets left="15.0" right="15.0" />
                            </padding>
                        </VBox>
                  <VBox layoutY="616.0" prefHeight="50.0" prefWidth="402.0">
                     <children>
                        <Button fx:id="myLoadTableBtn" mnemonicParsing="false" onAction="#loadTrainingEventData" prefHeight="25.0" prefWidth="374.0" text="Load Table">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </Button>
                        <Button fx:id="myReturnEventPageBtn" mnemonicParsing="false" prefHeight="25.0" prefWidth="374.0" text="Return Event Page">
                           <VBox.margin>
                              <Insets bottom="10.0" top="15.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                     <padding>
                        <Insets left="15.0" right="15.0" />
                     </padding>
                  </VBox>
                    </children>
                </AnchorPane>
            </content>
        </Tab>
    </TabPane>
</AnchorPane>
