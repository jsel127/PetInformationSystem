<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Pet.ReportController">
   <children>
      <LineChart fx:id="myLineChart" layoutX="565.0" layoutY="83.0" prefHeight="518.0" prefWidth="607.0" title="Training Report(Time in Day/Command Held Time)">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
        </yAxis>
      </LineChart>
      <Button layoutX="198.0" layoutY="155.0" mnemonicParsing="false" onAction="#generateLineChart" text="Generate Training Report" />
   </children>
</AnchorPane>
